    "svelte-check": "^2.2.6",
    "svelte-preprocess": "^4.10.1",
    "svelte-windicss-preprocess": "^4.2.6",
    "tslib": "^2.3.1",
    "typescript": "~4.6.2",
    "windicss": "^3.5.1"
